    <style type="text/css">
      div.body h2 {
        border-bottom: none;
        border-top: 1px dashed #336699;
        margin-top: 1.1em;
        padding-top: 0.9em;
        padding-bottom: 0.1em;
        line-height: 1.3em;
        font-size: 140%;
      }
      div.body p {
          margin-top: 0.6em;
      }
      div.body h2 a {
        color: #336699!important;
        text-decoration: none;
      }
      div.body h2 a:hover {
        text-decoration: underline;
      }
      div.body h2 .nolink {
        color: #888888;
      }
      div.body .author {
        color: #336699;
        font-size: 90%;
      }
      div.body .release {
        font-size: 68%;
      }
      div.body .downloads {
        margin-left: 0.1em;
        font-size: 68%;
