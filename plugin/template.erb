  <body>
    <div class="related">
      <h3>Navigation</h3>
      <ul>
        <li><a href="../doc/">fluentd documentation</a> &raquo;</li> 
      </ul>
    </div>
    <div class="document">
      <div class="documentwrapper">
        <div class="bodywrapper">
          <div class="body">
            <h1>Fluentd plugins</h1>

            <% plugins.each {|pl| %>
            <h2>
            <% if pl.url %>
              <a href="<%= pl.url %>" title="<%= h pl.name %>">
            <% else %>
              <span class="nolink">
            <% end %>
            <%= h pl.name %><br /><span class="release"><%= h pl.gemname %> <%= h pl.version %></span> <span class="downloads">[<%= h pl.downloads %> downloads]</span>
            <% if h pl.url %>
              </a>
            <% else %>
              </span>
            <% end %>
            </h2>
            <p><%= pl.summary %>
            <% if pl.author %>
            <span class="author">[<%= h pl.author %>]</span>
            <% end %>
            </p>
            <% } %>
          </div>
        </div>
      </div>
      <div class="clearer"></div>
    </div>

